  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3",
    "parcel": "^1.12.4"
  }
}
